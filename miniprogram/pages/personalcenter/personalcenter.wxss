.userinfo-avatar {
  border-radius: 150rpx;
  width: 200rpx;
  height: 200rpx;

}
page{
  background-color:#F5F5F5;
}
.color{
  background-color: white;
}
.b1{
  width:86vw;
  height:300rpx;
  padding: 0 7vw;
  margin-bottom: 50rpx;
  border-style:6px solid #F5F5F5;
  display: flex;
  justify-content: space-around;
  align-items: center
}
.name{
  line-height: 60rpx;
  width: 200rpx;
  font-size: 80rpx;
  text-align: center
}
.float{ 
  float:left;
}
.b2{
  width:100%;
  height:50rpx;
  border-style:6px solid #F5F5F5;
}
.text{
  margin-top:5rpx;
  padding-top: 35rpx;
  padding-bottom: 35rpx;
  padding-left: 50rpx;
}
.text1{
  padding-left: 50rpx;
}
.image{
  width:45rpx;
  height:45rpx;
}
.image2{
  width:60rpx;
  height:60rpx;
  padding-left: 400rpx;
}
.backbtn{
  width: 100%;
  padding-bottom: 0;
  background-color: whitesmoke;
}



.mine-provision {
  background-color: white;
  padding: 20rpx 40rpx;
  display: flex;
  flex-direction: row;
  margin-top: 10rpx;
  align-items: center;
}

.mine-provision view {
  color: #666666;
  font-size: 32rpx;
  flex-grow: 1;
}

.mine-provision image {
  width: 24rpx;
  height: 24rpx;
  flex-shrink: 0;
}
.feedback {
  margin: 0;
  padding: 0;
  background-color: transparent;
  width: 100%;
  height: 100%;
  display: flex;
  justify-content: start;
  align-items: center;
  color: #666666;
  font-size: 32rpx;
  font-weight: normal;
  flex-grow: 1;
}

.feedback::after {
  border: none;
}
