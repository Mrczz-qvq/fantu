/**index.wxss**/

/* 搜索框 */
.head{
  width: 100%;
  height: 10px;
}
.searchRoot {
  background: rgb(255, 255, 255);
  padding: 25rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}


.searchInput {
  flex: 1;
  height: 70rpx; 
  padding-left: 30rpx;
  border: 1px solid #f4c903;
  border-radius: 30rpx;
}

.searchImg {
  width: 70rpx;
  height: 70rpx;
  margin: 0 15rpx;
}

.category-panel1 {
  height: 200px;
  width: 100%;
  display: flex;
  color: #666;
  flex-direction: column;
  font-size: 11px;
  justify-content: space-around;
}

.category-panel2 {
  height: 80px;
  width: 100%;
  display: flex;
  color: #666;
  flex-direction: row;
  font-size: 11px;
  justify-content: space-around;
}

.category-item {
  display: flex;
  width: 50px;
  text-align: center;
  flex-direction: column;
}

.category-item-image {
  width: 40px;
  height: 40px;
  overflow: unset;
  padding: 15px 12px 0 6px;
}

.category-item-image1 {
  width: 50px;
  height: 45px;
  overflow: unset;
  padding: 15px 12px 0 6px;
}

.kf_button {
  background-color: rgba(255, 255, 255, 0);
  padding: 0px 0px 0px 0px;
  border: 0px;
}

.kf_button::after {
  border: 0px;
}

.category-item-text {
  margin-top: 11px;
}

.category-item-text1 {
  margin-top: 0px;
}
